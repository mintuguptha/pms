        "id": 2,
        "options": {
          "content": "<script>\n\nvar port = 8686; //DEFAULT\nvar host = window.location.host.split(':')[0]; //DEFAULT\nvar token = \"eyJrIjoic25MSjR2cWVQaWhNWDhmNWptdUFGTHFBTWt1VkE3amQiLCJuIjoiYXBpa2V5IiwiaWQiOjF9\";\n\n</script>\n\n<div class=\"text-center\">\n  <select style=\"height: auto;\" id=\"dashFolders\" class=\"btn btn-primary\"></select> \n  <select style=\"height: auto;\" id=\"dashUID\" class=\"btn btn-primary\"></select>\n  <button style=\"height: auto;\" id=\"generateBtn\" class=\"btn btn-primary\"><i class=\"fa fa-fw fa-file-text-o\"></i> Generate Report </button>\n</div>\n\n<script>\n\n$(document).ready(function(){\n  init();\n  getFolders();\n  $(\"#dashFolders\").change(function(){\n    $(\"#dashUID\").children('option:not(:first)').remove();\n    getDashboards($(\"#dashFolders\").val());\n  });\n});\n\nfunction init(){\n  $('#dashFolders').append(\"<option class='gf-form'> <span> <i class='fa fa-fw fa-th-large'> Select Folder </span> </option>\");\n  $('#dashUID').append(\"<option class='gf-form'> Select Dashboard </option>\");\n  $(\"#dashUID\").children('option:not(:first)').remove();\n}\n\nfunction getFolders(){ \n  $.ajax({\n    type: 'GET',\n    url: '/api/search',\n    data: 'query=&type=dash-folder',\n    dataType: 'json',\n    async: false,\n    success: function (data) {\n      $.each(data, function(index, el) {\n        $('#dashFolders').append(\"<option class='gf-form' value=\"+el.id+\">\"+el.title+\"</option>\");\n      });\n    }\n  }); \n}\n\nfunction getDashboards(id){\n  $.ajax({\n    type: 'GET',\n    url: '/api/search',\n    data: 'folderIds='+id+'&query=',\n    dataType: 'json',\n    async: false,\n    success: function (data) {\n      $.each(data, function(index, el) {\n        $('#dashUID').append(\"<option class='gf-form' value=\"+el.uid+\">\"+el.title+\"</option>\");\n      });\n    }\n  });\n}\n\n$(\"#generateBtn\").click(function(){\n  generate();\n});\n\nfunction generate(){\n  var uid = $(\"#dashUID\").val();\n  var url = \"http://\"+host+\":\"+port+\"/api/v5/report/\"+uid+\"?apitoken=\"+token;\n  window.open(url, '_blank',\"width=600,height=400\"); \n}\n\n\n</script>\n ",
